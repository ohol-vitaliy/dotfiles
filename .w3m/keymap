# Vim Hotkeys {{{

keymap  C-d     NEXT_HALF_PAGE
keymap  C-u     PREV_HALF_PAGE
keymap  C-h     SHIFT_LEFT
keymap  C-l     SHIFT_RIGHT
keymap  l       MOVE_RIGHT
keymap  RIGHT   MOVE_RIGHT
keymap  h       MOVE_LEFT
keymap  LEFT    MOVE_LEFT
keymap  DOWN    MOVE_DOWN1
keymap  j       MOVE_DOWN1
keymap  k       MOVE_UP1
keymap  UP      MOVE_UP1

keymap  0       LINE_BEGIN
keymap  $       LINE_END
keymap  zh      CENTER_H
keymap  zz      CENTER_V
keymap  gg      GOTO_LINE
keymap  gg      BEGIN
keymap  G       END
keymap  w       NEXT_LINK
keymap  b       PREV_LINK

keymap	SPC     GOTO_LINK
keymap	C-j     GOTO_LINK
keymap	t       TAB_LINK
keymap  C-n     NEXT_LINK
keymap  C-p     PREV_LINK

keymap	i       VIEW_IMAGE
keymap  ESC-C-j SUBMIT
keymap  pk      PEEK
keymap  pl      PEEK_LINK
keymap  pi      PEEK_IMG
keymap  gi      INFO
keymap	";"     MARK_URL
keymap	,       MARK_WORD

keymap  o     GOTO
keymap  O     TAB_GOTO
keymap  :e    LOAD

keymap  L     NEXT
keymap  H     PREV
keymap  K     NEXT_TAB
keymap  J     PREV_TAB

keymap  gv    VIEW
keymap  :w    SAVE
keymap  S     SAVE_LINK
keymap  C-r   RELOAD
keymap  r     RELOAD


keymap  n     SEARCH_NEXT
keymap  N     SEARCH_PREV
keymap  /     ISEARCH
keymap  ?     ISEARCH_BACK

keymap  q     QUIT
keymap  ZZ    QUIT
keymap  ZQ    EXIT
keymap	Q     EXIT

# }}}

# Scripts & CGI-BIN {{{
keymap  u       GOTO file:/cgi-bin/fn_restore_tab.cgi
keymap  d       GOTO file:/cgi-bin/fn_closetab_stash.cgi
keymap  y       GOTO file:/cgi-bin/fn_yank_page_url.cgi
keymap  Y       GOTO file:/cgi-bin/fn_yank_current_link.cgi
# keymap  gg      GOTO file:/cgi-bin/fn_goto_w3m_clipboard.cgi

keymap  xx      COMMAND "READ_SHELL ~/.w3m/cgi-bin/fnx_fzfmiru.cgi ; BACK ; GOTO file:/cgi-bin/fnx_execute.cgi ; BACK"
keymap  xu      COMMAND "READ_SHELL ~/.w3m/cgi-bin/fnx_change_user_agent.cgi ; BACK ; GOTO file:/cgi-bin/fnx_user_agent_set.cgi"
keymap  xr      GOTO file:/cgi-bin/fn_readerview_readability.cgi
# }}}

